{
  "redirects": [
    {
      "source": "/comece-agora.html",
      "destination": "/comece-agora",
      "permanent": true
    },
    {
      "source": "/success.html",
      "destination": "/success",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/comece-agora",
      "destination": "/comece-agora.html"
    },
    {
      "source": "/success",
      "destination": "/success.html"
    },
    { 
      "source": "/(.*)", 
      "destination": "/" 
    }
  ]
}